<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.kalfian.submissionMoviecatalogtvshow">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:fullBackupContent="@xml/backup_descriptor"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity android:name="com.kalfian.submissionMoviecatalogtvshow.Activity.DetailTvActivity">

        </activity>
        <activity
                android:name="com.kalfian.submissionMoviecatalogtvshow.Activity.DetailMovieActivity"
                tools:ignore="InnerclassSeparator">

        </activity>
        <activity
                android:name="com.kalfian.submissionMoviecatalogtvshow.Activity.MainActivity"
                tools:ignore="InnerclassSeparator"
        >
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>